                      "nullable": false
                    },
                    "defaultValueComputed": "uuid_generate_v4()"
                  }
                },
                {
                  "column": {
                    "name": "email",
                    "type": "TEXT",
                    "constraints": {
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "department",
                    "type": "TEXT"
                  }
                },
                {
                  "column": {
                    "name": "employmentType",
                    "type": "employmenttype",
                    "constraints": {
                      "nullable": false
                    },
                    "defaultValueComputed": "'full_time'::employmenttype"
                  }
                },
                {
                  "column": {
                    "name": "workPattern",
                    "type": "workpattern",
                    "constraints": {
                      "nullable": false
